{
  "name": "react-rematch-ssr",
  "version": "1.0.0",
  "description": "基于react、react-router、rematch、webpack等构建的服务端渲染模板",
  "main": "index.js",
  "scripts": {
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.conf.client.js",
    "dev:server": "cross-env NODE_ENV=development nodemon server/index.js",
    "clear": "rimraf dist",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.conf.client.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.conf.server.js --progress --hide-modules",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "lint": "eslint --ext .jsx --ext .js src/",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Kim09",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@loadable/babel-plugin": "^5.0.1",
    "@loadable/webpack-plugin": "^5.2.1",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-css-modules": "^3.4.2",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "ejs-compiled-loader": "^1.1.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^2.0.0",
    "hash-sum": "^1.0.2",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.0",
    "husky": "^1.0.1",
    "ignore-loader": "^0.1.2",
    "lint-staged": "^7.3.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.2",
    "nodemon": "^1.18.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "react-hot-loader": "^4.3.8",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "sugarss": "^2.0.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
