    "@rematch/loading": "^1.1.0",
    "axios": "^0.18.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "redux-connect": "^8.0.0",
    "regenerator-runtime": "^0.12.1",
    "serialize-javascript": "^1.5.0"
  }
}
